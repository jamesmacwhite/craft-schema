{
    "name": "jamesmacwhite/craft-schema",
    "description": "A fluent builder Schema.org types and ld+json generator based on Spatie's schema-org package",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "schema"
    ],
    "support": {
        "docs": "https://github.com/jamesmacwhite/craft-schema/blob/master/README.md",
        "issues": "https://github.com/jamesmacwhite/craft-schema/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rias",
            "homepage": "https://rias.be"
        },
        {
            "name": "James White",
            "homepage": "https://www.jmwhite.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^5.0.0",
        "spatie/schema-org": "^3.23"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
          "jamesmacwhite\\schema\\": "src/"
        }
    },
    "extra": {
        "name": "JSON-LD Schema",
        "handle": "schema",
        "schemaVersion": "1.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/jamesmacwhite/craft-schema/master/CHANGELOG.md",
        "class": "jamesmacwhite\\schema\\Schema"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
